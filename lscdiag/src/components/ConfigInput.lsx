import React from "react"

import { Tool } from './ToolConfig.lsx'

import compose from 'recompose/compose'
import pure from 'recompose/pure'
import withPropsFromObservables from 'hawks/lib/withPropsFromObservables'

ConfigInput = compose(
  pure
  withPropsFromObservables! props -> ({
    config: props.fixture.toolchainDescriptor$
  })
)((props) ->
  <div>
    <Tool descriptor={props.config} fixture={props.fixture} />
  </div>
)

export default ConfigInput
